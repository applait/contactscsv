{
    "version": "0.0.1",
    "name": "Appname",
    "description": "Add app description",
    "launch_path": "/index.html",
    "icons": {
        "48": "/img/icons/48.png",
        "60": "/img/icons/60.png",
        "128": "/img/icons/128.png",
        "512": "/img/icons/512.png"
    },
    "developer": {
        "name": "Applait",
